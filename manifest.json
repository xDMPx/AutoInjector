{
    "manifest_version": 3,
    "name": "AutoInjector",
    "description": "Automatic injection of scripts and CSS",
    "version": "0.1.0",
    "background": {
        "scripts": ["background.js"],
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "{632c8b71-85ce-4941-983f-3aac02d58830}"
        }
    }
}
